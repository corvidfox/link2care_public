IF (FSS2_V5 = 0 OR FSS2_V5 = 1) FSSb_V5 = 1.
IF (FSS2_V5 = 2 OR FSS2_V5 = 99) FSSb_V5 = 0.
VARIABLE LABELS FSSb_V5 'Recode into yes or no'.
VALUE LABELS FSSb_V5 0 'no' 1 'affirmative yes'.
EXECUTE. 

IF (FSS3_V5 = 1) FSSc_V5 = 1.
IF (FSS3_V5 = 0 OR FSS3_V5 = 9) FSSc_V5 = 0.
VARIABLE LABELS FSSc_V5 'Recode into yes or no'.
VALUE LABELS FSSc_V5 0 'no' 1 'affirmative yes'.
EXECUTE. 

IF (FSS4_V5 = 1) FSSd_V5 = 1.
IF (FSS4_V5= 0 OR FSS4_V5 = 9) FSSd_V5 = 0.
VARIABLE LABELS FSSd_V5 'Recode into yes or no'.
VALUE LABELS FSSd_V5 0 'no' 1 'affirmative yes'.
EXECUTE. 

IF (FSS5_V5 = 1) FSSe_V5 = 1.
IF (FSS5_V5 = 0 OR FSS5_V5 = 9) FSSe_V5 = 0.
VARIABLE LABELS FSSe_V5 'Recode into yes or no'.
VALUE LABELS FSSe_V5 0 'no' 1 'affirmative yes'.
