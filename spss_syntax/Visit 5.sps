*BMI.
COMPUTE weight_kg_V5 = WEIGHT_5 * 0.453592.
VARIABLE LABELS weight_kg_V5 'Weight in kilograms'. 
EXECUTE.

COMPUTE height_meter_V5= HEIGHT_V5 * 0.01.
VARIABLE LABELS height_meter_V5 'Height in meters'.
EXECUTE.

COMPUTE height_meter_sq_V5= height_meter_V5 * height_meter_V5.
VARIABLE LABELS height_meter_sq_V5 'Height in meters squared'.
EXECUTE.

COMPUTE BMI_V5= weight_kg_v5 / height_meter_sq_V5.
VARIABLE LABELS BMI_V5 'Body mass index'.
EXECUTE.

IF (bmi_V5<18.5) weight_status_V5=0.
IF (bmi_V5>=18.5 and bmi_V5<=24.99) weight_status_V5=1.
IF (bmi_V5>=25 and bmi_V5<=29.99) weight_status_V5=2.
IF (bmi_V5>=30) weight_status_V5=3.
VARIABLE LABELS weight_status_V5 'Weight categroized into underweight, normal, overweight, and obese'.
VALUE LABELS weight_status_V5 0 'underweight' 1 'normal' 2 'overweight' 3 'obese'.
EXECUTE.

IF (weight_status_V5= 0 or weight_status_V5= 1 or weight_status_V5= 2) obese_V5 =0.
IF (weight_status_V5 = 3) obese_V5=1.
VARIABLE LABELS obese_V5 'Obese'. 
VALUE LABELS obese_V5 0 'not obese' 1 'obese'.
EXECUTE.

IF (weight_status_V5= 0 or weight_status_V5= 1) ovrwt_obese_V5 =0.
IF (weight_status_V5= 2 or weight_status_V5= 3) ovrwt_obese_V5=1.
VARIABLE LABELS ovrwt_obese_V5 'Overweight or obese'.
VALUE LABELS ovrwt_obese_V5 0 'not overweight or obese' 1 'overweight or obese'.
EXECUTE.

**MINI Mental State Exam

DATASET ACTIVATE DataSet1.
COMPUTE mms_score_V5 = MMS_1_V5 + MMS_2_V5 + RECAL1_V5 +COUNT_V5 + RECAL2_V5 + OBJECT_V5 + PAPER_V5 + MMS_11_V5 + MMS_10_V5 + MMS_9_V5 + MMS_7_V5.
VARIABLE LABELS mms_score_V5 'MINI Mental State Exam score when participant is unable to count backwards from 100 by 7s and  are asked to spell (world) backwards'. 
EXECUTE.

IF (Count_V5 > 0) MMS_total_V5 = MMS_Score_V5.
IF (Count_V5 = 0) MMS_total_V5 = MMS_S_V5.
VARIABLE LABELS MMS_total_V5 'MINI Mental State Exam total score'. 
EXECUTE.

RECODE MMS_total_V5 (Lowest thru 17=2) (18 thru 23=1) (24 thru Highest=0) INTO MMS_severity_V5. 
VARIABLE LABELS  MMS_severity_V5 'What is the liklihood of cognitive impairment?'. 
VALUE LABELS MMS_severity_V5 0 'No cognitive impairment' 1 'Mild cognitive impairment' 2 'Severe cognitive impairment'. 
EXECUTE. 




*PHQ Depression

RECODE PHQ1_V5 PHQ2_V5 PHQ3_V5 PHQ4_V5 PHQ5_V5  PHQ6_V5 PHQ7_V5 PHQ8_V5  (Lowest thru 1=0) (2 thru 
    Highest=1) INTO PHQ1_V5_dicot PHQ2_V5_dicot PHQ3_V5_dicot PHQ4_V5_dicot PHQ5_V5_dicot PHQ6_V5_dicot 
    PHQ7_V5_dicot PHQ8_V5_dicot. 
VARIABLE LABELS 
PHQ1_V5_dicot 'Little interest or pleasure in doing things dicot' 
/PHQ2_V5_dicot 'Felling down, depressed, or hopeless dicot' 
/PHQ3_V5_dicot 'Trouble falling or staying asleep, or sleeping too much dicot' 
/PHQ4_V5_dicot 'Feeling tired or having little energy dicot' 
/PHQ5_V5_dicot ' Poor appetite or overeating dicot' 
/PHQ6_V5_dicot 'Failure or have let yourself or your family down dicot' 
/PHQ7_V5_dicot ' Reading the newspaper or watching television' 
/PHQ8_V5_dicot 'Fidgety or restless dicot'. 
VALUE LABELS PHQ1_V5_dicot to PHQ8_V5_dicot
0 'Not at all and several days' 1 'More than half the days and nearly every day'.
EXECUTE.
 
COMPUTE PHQ_dep_dicot_total_V5 =PHQ1_V5_dicot + PHQ2_V5_dicot + PHQ3_V5_dicot + PHQ4_V5_dicot + 
    PHQ5_V5_dicot + PHQ6_V5_dicot + PHQ7_V5_dicot + PHQ8_V5_dicot. 
VARIABLE LABELS PHQ_dep_dicot_total_V5  'PHQ depression dichotomized total'.
EXECUTE.

IF (PHQ1_V5_dicot = 1 or PHQ2_V5_dicot = 1) PHQsymp_V5  = 1.
IF (PHQ1_V5_dicot = 0 and PHQ2_V5_dicot = 0) PHQsymp_V5 = 0.
VARIABLE LABELS PHQsymp_V5 'Does PHQ1 or PHQ2 have more than half the days and nearly every day selected?'.
VALUE LABELS PHQsymp_V5 0 'No' 1 'Yes'.
EXECUTE. 

IF (PHQsymp_V5 = 1 and PHQ_dep_dicot_total_V5 > 4) PHQmdd_V5  = 1.
IF (PHQsymp_V5 = 0) PHQmdd_V5 = 0.
IF (PHQ_dep_dicot_total_V5 < 5) PHQmdd_V5 =0.
VARIABLE LABELS PHQmdd_V5 'Is there probable major depressive disorder?'.
VALUE LABELS PHQmdd_V5 0 'No' 1 'Yes'. 
EXECUTE.

